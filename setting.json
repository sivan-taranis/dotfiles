{
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-v>": false,
    "<C-c>": false,
    "<C-x>": false,
    "<C-b>": false,
    "<C-j>": false,
  },
  "vim.sneak": true,
  "vim.easymotion": true,
  "vim.useCtrlKeys": true,
  "vim.vimrc.enable": true,
  "vim.leader": "<space>",
  "vim.vimrc.path": "C:\\Users\\Taranis\\.vimrc",
  "vim.sneakUseIgnorecaseAndSmartcase":true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["6", "j"]
    },
    {
      "before": ["K"],
      "after": ["6", "k"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": [
        "<Leader>",","
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<Leader>","l"
      ],
      "commands": [
        "workbench.action.navigateForward"
      ]
    },
    {
      "before": [
        "<Leader>","h"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    { 
      "before": [
        "<Leader>","."
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<Leader>","d"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<Leader>","g"
      ],
      "commands": [
        "magit.status"
      ]
    },
    {
      "before": [
        "leader","b","s"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": ["leader", "s"],
      "after": ["leader", "leader", "s"]
    },
    {
      "before": ["leader", "w"],
      "after": ["leader", "leader", "w"]
    },
    {
      "before": ["leader", "b"],
      "after": ["leader","leader","b"]
    },
    {
      "before": ["leader", "k"],
      "after": ["leader", "leader", "k"]
    },
    {
      "before": ["leader", "j"],
      "after": ["leader", "leader", "j"]
    },
    {
      "before": ["<leader>", "e"],
      "after": ["<leader>", "<leader>", "e"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "f"],
      "after": ["<leader>", "<leader>", "f"]
    },
    {
      "before": ["leader", "n"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["leader", "N"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.commentLine"]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": [
            "p",
        ],
        "after": [
            "p",
            "g",
            "v",
            "y"
        ]
    }
  ],
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.colorCustomizations": {
    "[Tokyo Night]": { // or "[Tokyo Night Storm]"
      "editorCodeLens.foreground": "#7982a9", // Preferred hex color
      "editor.findMatchHighlightBackground": "#7982a9",
      "editor.selectionHighlightBorder": "#474e71",
      "editor.selectionBackground": "#4b0808",

      "editor.wordHighlightBorder": "#7982a9",
      //"comments": "#d4922f"

      "editor.wordHighlightStrongBorder": "#7982a9",
      //"editorBracketMatch.border": "#7982a9", // brackets
      // "editorBracketMatch.background": "#7982a9", // brackets
    }
  },
  "editor.tokenColorCustomizations": {
    "[Tokyo Night]": {
      "comments": "#4d6f53"
    }
  },
  "git.ignoreMissingGitWarning": true,

  
}
